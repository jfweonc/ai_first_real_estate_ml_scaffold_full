{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "P2 Normalize Output Row",
  "type": "object",
  "required": ["listing_key", "domain", "obs_ts", "addr_std", "zip", "price_list", "beds", "baths", "sqft"],
  "properties": {
    "listing_key": {"type": "string"},
    "domain": {"type": "string"},
    "obs_ts": {"type": "string", "format": "date-time"},
    "addr_std": {"type": "string"},
    "zip": {"type": "string", "pattern": "^\\d{5}(-\\d{4})?$"},
    "price_list": {"type": "number", "minimum": 0},
    "beds": {"type": "integer", "minimum": 0},
    "baths": {"type": "number", "minimum": 0},
    "sqft": {"type": ["number", "null"], "minimum": 0}
  }
}
